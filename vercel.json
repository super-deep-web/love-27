{
  "version": 2,
  "builds": [
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/carta",
      "dest": "/public/carta.html"
    },
    {
      "src": "/game.js",
      "dest": "/public/game.js"
    },
    {
      "src": "/letter.js",
      "dest": "/public/letter.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}